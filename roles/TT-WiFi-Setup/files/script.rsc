/interface wireless security-profiles set [ find default=yes ] authentication-types=wpa2-psk mode=dynamic-keys  wpa2-pre-shared-key="CHANGE_ME"
if ([/interface wireless security-profiles find name="service-net" ] ="") do={/interface wireless security-profiles add authentication-types=wpa2-psk mode=dynamic-keys name=service-net wpa2-pre-shared-key="CHANGE_ME"}
if ([/interface wireless find where name="service-net" ] ="") do={/interface wireless add disabled=no master-interface=[/interface wireless get number=[/interface wireless find where interface-type!="virtual-AP"] name] name=service-net security-profile=service-net ssid=service-net}
if ([/interface bridge port find bridge=[/interface get number=[/interface find where type=bridge] name] interface=service-net]="") do={/interface bridge port add bridge=[/interface get number=[/interface find where type=bridge] name] interface=service-net}
if ([/interface wireless get number=[/interface wireless find where interface-type!="virtual-AP"] frequency]!="auto") do={/interface wireless set number=[/interface wireless find where interface-type!="virtual-AP"] frequency=auto }
if ([/interface wireless get number=[/interface wireless find where interface-type!="virtual-AP"] tx-power]!="14") do={/interface wireless set number=[/interface wireless find where interface-type!="virtual-AP"] tx-power=14 tx-power-mode="all-rates-fixed" }
if ([/interface wireless get number=[/interface wireless find where interface-type!="virtual-AP"] "tx-power-mode"]!="all-rates-fixed") do={/interface wireless set number=[/interface wireless find where interface-type!="virtual-AP"] tx-power=14 tx-power-mode="all-rates-fixed" }
